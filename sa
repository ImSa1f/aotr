    function GetCorner(sz, pos, anch)
        pos = typeof(pos) == 'UDim2' and Vector2.new(pos.X.Offset, pos.Y.Offset) or pos
        anch = anch or Vector2.new(0,0)
        return pos+(sz*anch)
    end
    
